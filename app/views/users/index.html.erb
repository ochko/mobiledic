<h2 id="title">Users</h2>

<table>
  <tr>
  <th>ID</th>
  <th>Login</th>
  <th>E-mail</th>
  <th>State</th>
  <th>Last activity</th>
  <th>Quiz count</th>
</tr>
  <% for user in @users -%>
  <tr>
    <td><%= user.id -%></td>
    <td><%= user.login -%></td>
    <td><%= user.email -%></td>
    <td><%= user.state -%></td>
    <td><%= user.last_activity_at -%></td>
    <td><%= user.learn_processes.size*3 -%></td>
  </tr>
  <% end -%>
</table>
